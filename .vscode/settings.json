{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "python.terminal.activateEnvironment": true,
    "python.defaultInterpreterPath": "${env:VIRTUAL_ENV}",
    "markdown.extension.toc.updateOnSave": false,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#5073ad",
        "activityBar.background": "#5073ad",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#d7a8b9",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#5073ad",
        "statusBar.background": "#405c8a",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#5073ad",
        "statusBarItem.remoteBackground": "#405c8a",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#405c8a",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#405c8a99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#405c8a",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/*.egg-info": true
    }
}